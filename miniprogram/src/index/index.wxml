<view class="container">
    <xr-canvas id="xr-canvas" wx:if="{{xrShow}}" width="{{renderWidth}}" height="{{renderHeight}}" style="width:{{width}}px;height:{{height}}px;display:block;z-index: 99; " bind:handleAssetsLoaded="loadingChange" bind:handleAssetsProgress="loadingProgress" bind:stayPage="stayPage"></xr-canvas>
    <view class="error" wx:if="{{unSupport}}">{{unSupport}}</view>
    <view class="p_guide" wx:if="{{p_guideFlag}}">
        <image class="p_guideFlag_image" wx:if="{{workflowData.image_url}}" src="{{workflowData.p_guide.image_url}}" mode="" />
        <video wx:if="{{workflowData.video_url}}" src="{{workflowData.p_guide.video_url}}" />
    </view>
    <view class="p_scan" wx:if="{{p_scanFlag}}">
        <image class="p_scan_image" src="{{workflowData.p_scan.image_url}}" mode="" />
    </view> 
    <view class="p_loading" wx:if="{{p_loadingFlag}}">
        <image class="p_loading_image" src="{{workflowData.p_loading.image_url}}" mode="" />
    </view> 
    <view class="p_ending" wx:if="{{p_endingFlag}}">
        <view class="p_ending_image" src="{{workflowData.p_ending.text}}"/>
    </view> 
</view>