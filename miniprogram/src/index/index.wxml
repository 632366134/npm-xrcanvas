<view class="container">
    <xr-canvas id="xr-canvas" wx:if="{{xrShow}}" width="{{renderWidth}}" height="{{renderHeight}}" style="width:{{width}}px;height:{{height}}px;display:block;z-index: 99; " bind:handleAssetsLoaded="loadingChange" bind:handleAssetsProgress="loadingProgress" bind:stayPage="stayPage" workflowType="{{workflowType}}" p_arData="{{p_arData}}" workflowData="{{workflowData}}" bind:showInteractMedia="showInteractMedia"></xr-canvas>
    <view class="error" wx:if="{{unSupport}}">{{unSupport}}</view>
    <view class="p_guide" wx:if="{{p_guideFlag}}">
        <image class="p_guideFlag_image" wx:if="{{workflowData.p_guide.image_url}}" src="{{workflowData.p_guide.image_url}}" mode="" />
        <video autoplay class="p_guideFlag_video" wx:if="{{workflowData.p_guide.video_url}}" src="{{workflowData.p_guide.video_url}}" />
    </view>
    <view class="p_scan" wx:if="{{p_scanFlag}}">
        <image class="p_scan_image" src="{{workflowData.p_scan.image_url}}" mode="aspectFill" />
    </view>
    <view class="p_loading" wx:if="{{p_loadingFlag}}">
        <image class="p_loading_image" src="{{workflowData.p_loading.image_url}}" mode="" />
    </view>
    <view class="p_ending" wx:if="{{p_endingFlag}}">
        <view class="p_ending_image" src="{{workflowData.p_ending.text}}" />
    </view>
    <view class="p_ending" wx:if="{{eventFlag}}" bind:tap="eventFlagChange">
        <image class="p_guideFlag_image" wx:if="{{event_image}}" src="{{event_image}}" mode="aspectFit" />
        <video class="p_guideFlag_video" wx:if="{{event_url}}" src="{{event_url}}" />
    </view>
</view>